{
  "name": "discord_areena_bot",
  "version": "1.0.0",
  "engines": {
    "node": "14.x",
    "yarn": "1.x"
  },
  "scripts": {
    "build-bot": "tsc --build tsconfig.json",
    "start-bot": "node dist/bot/index.js",
    "build-preview": "webpack --mode production",
    "start-preview": "node server.js",
    "start-bot-dev": "nodemon",
    "debug-bot": "node --inspect-brk -r ts-node/register ./src/bot/index.ts",
    "start-preview-dev": "start http://localhost:8080/ && webpack-dev-server --hot --mode development",
    "benchmark-winrate": "jest -c jest_benchmark.config.js",
    "benchmark-image": "ts-node ./src/benchmarks/benchmarkImage.ts",
    "benchmark-draw": "ts-node ./src/benchmarks/benchmarkDraw.ts",
    "lint": "eslint . --ext .tsx,.ts,.js",
    "test": "jest",
    "heroku-postbuild": "yarn test-render && yarn build-bot && yarn build-preview",
    "test-render": "ts-node ./src/benchmarks/renderTest.ts",
    "update-commands": "ts-node ./src/bot/updateCommands.ts",
    "clear-commands": "ts-node ./src/bot/clearCommands.ts"
  },
  "repository": "git@github.com:vilvai/discord_areena_bot.git",
  "author": "ville-pc <ville.vainio1@gmail.com>",
  "license": "GPLV3",
  "private": true,
  "dependencies": {
    "canvas": "^2.6.1",
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fluent-ffmpeg": "^2.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/fluent-ffmpeg": "^2.1.14",
    "@types/html-webpack-plugin": "^3.2.3",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.1",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/rimraf": "^3.0.0",
    "@types/styled-components": "^5.1.0",
    "@types/webpack": "^4.41.13",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.0.1",
    "nodemon": "^2.0.6",
    "prettier": "^2.0.5",
    "style-loader": "^1.2.1",
    "ts-jest": "^26.0.0",
    "ts-loader": "^7.0.4",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.2",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
